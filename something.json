{
  "executor": "python",
  "runnable": "main.py",
  "cuda": [
    4, 5, 6, 7
  ],
  "concurrency": 4,
  "configs": {
    "==base==": {
      "--max_span_length": 10,
      "--max_sentence_length": 120,
      "--char_count_gt": 0,
      "--bichar_count_gt": 2,
      "--bichar_emb_size": 0,
      "--seg_emb_size": 25,
      "--char_emb_size": 50,
      "--char_emb_pretrain": "word2vec/lattice_lstm/gigaword_chn.all.a2b.uni.ite50.vec",
      "--bichar_emb_pretrain": "off",
      "--rnn_cell_type": "lstm",
      "--rnn_num_layer": 2,
      "--rnn_hidden": 256,
      "--frag_type": "rnn",
      "--load_from_cache": "True",
      "--train_on": "True",
      "--use_data_set": "full_train"
    },
    "==more==": [
      {
        "--token_type": "plain"
      },
      {
        "--token_type": "tfer",
        "--tfer_num_layer": [2, 4, 8]
      }
    ]
  }
}
